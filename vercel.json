{
    "version": 2,
    "builds": [
        {
            "src": "src/app.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/streams",
            "destination": "/src/app.js"
        },
        {
            "source": "/(.*)",
            "destination": "/src/app.js"
        }
    ]
}